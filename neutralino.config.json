{
    "applicationId": "com.servermanager.app",
    "version": "1.0.0",
    "defaultMode": "window",
    "port": 0,
    "documentRoot": "/resources",
    "url": "/index.html",
    "enableServer": false,
    "enableNativeAPI": true,
    "tokenSecurity": "one-time",
    "logging": {
        "enabled": true,
        "writeToLogFile": true
    },
    "modes": {
        "window": {
            "title": "Server Manager",
            "width": 800,
            "height": 600,
            "minWidth": 800,
            "minHeight": 600,
            "center": true,
            "icon": "/resources/icons/server-manager.ico",
            "enableInspector": false
        }
    },
    "cli": {
        "binaryName": "ServerManager",
        "resourcesPath": "/resources/",
        "extensionsPath": "/extensions/",
        "clientLibrary": "/resources/js/neutralino.js"
    }
}
